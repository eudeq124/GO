{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GUNICORN_CMD_ARGS": "--worker-class gevent",
    "SECRET_KEY": "vercel_secret_key",
    "EMAIL_USER": "",
    "EMAIL_PASSWORD": ""
  }
}
